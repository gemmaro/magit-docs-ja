msgstr "magit-diff-visit-file-other-window"

#. type: deffn
#: sed-out/magit.texi:5303
#, no-wrap
msgid "Command magit-diff-visit-file-other-window"
msgstr "Command magit-diff-visit-file-other-window"

#. type: cindex
#: sed-out/magit.texi:5305
#, no-wrap
msgid "magit-diff-visit-file-other-frame"
msgstr "magit-diff-visit-file-other-frame"

#. type: deffn
#: sed-out/magit.texi:5306
#, no-wrap
msgid "Command magit-diff-visit-file-other-frame"
msgstr "Command magit-diff-visit-file-other-frame"

#. type: cindex
#: sed-out/magit.texi:5308
#, no-wrap
