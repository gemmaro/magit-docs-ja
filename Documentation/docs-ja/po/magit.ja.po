msgid "C-c C-r"
msgstr "C-c C-r"

#. type: cindex
#: sed-out/magit.texi:6334
#, no-wrap
msgid "git-commit-review"
msgstr "git-commit-review"

#. type: item
#: sed-out/magit.texi:6335
#, no-wrap
msgid "@kbd{C-c C-r} @tie{}@tie{}@tie{}@tie{}(@code{git-commit-review})"
msgstr "@kbd{C-c C-r} @tie{}@tie{}@tie{}@tie{}(@code{git-commit-review})"

#. type: table
#: sed-out/magit.texi:6338
msgid "Insert a header acknowledging that you have reviewed the commit."
msgstr ""
"あなたがコミットをレビューしたことを承認(acknowledge)するヘッダーを挿入しま"
"す。"

#. type: kindex
#: sed-out/magit.texi:6339
#, no-wrap
msgid "C-c C-s"
msgstr "C-c C-s"

#. type: cindex
#: sed-out/magit.texi:6340
#, no-wrap
msgid "git-commit-signoff"
msgstr "git-commit-signoff"

#. type: item
#: sed-out/magit.texi:6341
#, no-wrap
msgid "@kbd{C-c C-s} @tie{}@tie{}@tie{}@tie{}(@code{git-commit-signoff})"
msgstr "@kbd{C-c C-s} @tie{}@tie{}@tie{}@tie{}(@code{git-commit-signoff})"

#. type: table
#: sed-out/magit.texi:6344
msgid "Insert a header to sign off the commit."
msgstr "ヘッダーを挿入して、コミットを承認(sign off)します。"

#. type: cindex
#: sed-out/magit.texi:6346
#, no-wrap
msgid "git-commit-test"
msgstr "git-commit-test"

#. type: item
#: sed-out/magit.texi:6347
#, no-wrap
msgid "@kbd{C-c C-t} @tie{}@tie{}@tie{}@tie{}(@code{git-commit-test})"
msgstr "@kbd{C-c C-t} @tie{}@tie{}@tie{}@tie{}(@code{git-commit-test})"

#. type: table
#: sed-out/magit.texi:6350
