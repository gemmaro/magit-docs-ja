msgstr "@kbd{! a} @tie{}@tie{}@tie{}@tie{}(@code{magit-run-gitk-all})"

#. type: table
#: sed-out/magit.texi:2618
msgid "This command runs @code{gitk --all} in the current repository."
msgstr "このコマンドは、現在のリポジトリで@code{gitk --all}を実行します。"

#. type: kindex
#: sed-out/magit.texi:2619
#, no-wrap
msgid "! b"
msgstr "! b"

#. type: cindex
#: sed-out/magit.texi:2620
#, no-wrap
msgid "magit-run-gitk-branches"
msgstr "magit-run-gitk-branches"

#. type: item
#: sed-out/magit.texi:2621
#, no-wrap
msgid "@kbd{! b} @tie{}@tie{}@tie{}@tie{}(@code{magit-run-gitk-branches})"
msgstr "@kbd{! b} @tie{}@tie{}@tie{}@tie{}(@code{magit-run-gitk-branches})"

#. type: table
#: sed-out/magit.texi:2624
