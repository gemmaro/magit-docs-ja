msgstr "C-c C-i"

#. type: cindex
#: sed-out/magit.texi:6322
#, no-wrap
msgid "git-commit-insert-pseudo-header"
msgstr "git-commit-insert-pseudo-header"

#. type: item
#: sed-out/magit.texi:6323
#, no-wrap
msgid "@kbd{C-c C-i} @tie{}@tie{}@tie{}@tie{}(@code{git-commit-insert-pseudo-header})"
msgstr "@kbd{C-c C-i} @tie{}@tie{}@tie{}@tie{}(@code{git-commit-insert-pseudo-header})"

#. type: table
#: sed-out/magit.texi:6326
msgid "Insert a commit message pseudo header."
msgstr "コミットメッセージの疑似ヘッダーを挿入します。"

#. type: kindex
#: sed-out/magit.texi:6327
#, no-wrap
msgid "C-c C-a"
msgstr "C-c C-a"

#. type: cindex
#: sed-out/magit.texi:6328
#, no-wrap
msgid "git-commit-ack"
msgstr "git-commit-ack"

#. type: item
#: sed-out/magit.texi:6329
#, no-wrap
msgid "@kbd{C-c C-a} @tie{}@tie{}@tie{}@tie{}(@code{git-commit-ack})"
msgstr "@kbd{C-c C-a} @tie{}@tie{}@tie{}@tie{}(@code{git-commit-ack})"

#. type: table
#: sed-out/magit.texi:6332
msgid "Insert a header acknowledging that you have looked at the commit."
msgstr ""
"あなたがコミットをチェックしたことを承認(acknowledge)するヘッダーを挿入しま"
"す。"

#. type: kindex
#: sed-out/magit.texi:6333
