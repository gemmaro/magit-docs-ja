#: sed-out/magit.texi:4033 sed-out/magit.texi:4506
msgid ""
"the <a href=\"http://git-scm.com/docs/git-diff\">git-diff(1)</a> manpage."
msgstr ""
"the <a href=\"http://git-scm.com/docs/git-diff\">git-diff(1)</a> manpage."

#. type: iftex
#: sed-out/magit.texi:4036 sed-out/magit.texi:4509
msgid "the git-diff(1) manpage."
msgstr "the git-diff(1) manpage."

