msgstr "@code{magit-wip-after-apply-mode}のモードラインライター(lighter)"

#. type: defopt
#: sed-out/magit.texi:9805
#, no-wrap
msgid "magit-wip-before-change-mode-lighter"
msgstr "magit-wip-before-change-mode-lighter"

#. type: defopt
#: sed-out/magit.texi:9808
msgid "Mode-line lighter for @code{magit-wip-before-change-mode}."
msgstr "@code{magit-wip-before-change-mode}のモードラインライター(lighter)"

#. type: defopt
