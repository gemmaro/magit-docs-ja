#: sed-out/magit.texi:1486
#, no-wrap
msgid "M-<tab>"
msgstr ""

#. type: cindex
#: sed-out/magit.texi:1487
#, no-wrap
msgid "magit-section-cycle-diffs"
msgstr ""

#. type: item
#: sed-out/magit.texi:1488
#, no-wrap
msgid "@kbd{M-<tab>} @tie{}@tie{}@tie{}@tie{}(@code{magit-section-cycle-diffs})"
msgstr ""

#. type: table
#: sed-out/magit.texi:1491
msgid "Cycle the visibility of diff-related sections in the current buffer."
msgstr ""

#. type: kindex
#: sed-out/magit.texi:1492
#, no-wrap
msgid "S-<tab>"
msgstr ""

#. type: cindex
#: sed-out/magit.texi:1493
#, no-wrap
msgid "magit-section-cycle-global"
msgstr ""

#. type: item
#: sed-out/magit.texi:1494
#, no-wrap
msgid "@kbd{S-<tab>} @tie{}@tie{}@tie{}@tie{}(@code{magit-section-cycle-global})"
msgstr ""

#. type: table
#: sed-out/magit.texi:1497
msgid "Cycle the visibility of all sections in the current buffer."
msgstr ""

#. type: kindex
#: sed-out/magit.texi:1498
#, no-wrap
msgid "1"
msgstr ""

#. type: cindex
#: sed-out/magit.texi:1499
#, no-wrap
msgid "magit-section-show-level-1"
msgstr ""

#. type: item
#: sed-out/magit.texi:1500
#, no-wrap
msgid "@kbd{1} @tie{}@tie{}@tie{}@tie{}(@code{magit-section-show-level-1})"
msgstr ""

#. type: kindex
#: sed-out/magit.texi:1501
#, no-wrap
msgid "2"
msgstr ""

#. type: cindex
#: sed-out/magit.texi:1502
#, no-wrap
msgid "magit-section-show-level-2"
msgstr ""

#. type: item
