#. type: kindex
#: sed-out/magit.texi:5360 sed-out/magit.texi:9949
#, no-wrap
msgid "C-c M-g e"
msgstr ""

#. type: cindex
#: sed-out/magit.texi:5361 sed-out/magit.texi:5364
#, no-wrap
msgid "magit-blame-echo"
msgstr ""

#. type: item
#: sed-out/magit.texi:5362
#, no-wrap
msgid "@kbd{C-c M-g e} @tie{}@tie{}@tie{}@tie{}(@code{magit-blame-echo})"
msgstr ""

#. type: kindex
#: sed-out/magit.texi:5363
#, no-wrap
msgid "C-c M-g B e"
msgstr ""

#. type: item
#: sed-out/magit.texi:5365
#, no-wrap
msgid "@kbd{C-c M-g B e} @tie{}@tie{}@tie{}@tie{}(@code{magit-blame-echo})"
